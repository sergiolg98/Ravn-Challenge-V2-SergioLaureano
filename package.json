{
  "name": "ravn-challenge",
  "version": "1.0.0",
  "description": "API for an sports store",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only --watch src src/server.ts",
    "build": "tsc",
    "migrate": "npx prisma migrate deploy",
    "migrate:dev": "npx prisma migrate dev --name init",
    "seed": "ts-node prisma/seeds.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "check-format": "prettier --check \"src/**/*.ts\""
  },
  "author": "Sergio Laureano",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.627.0",
    "@prisma/client": "^5.18.0",
    "awilix": "^10.0.2",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.1.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^29.7.0",
    "prisma": "^5.18.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4",
    "eslint-plugin-prettier": "^5.0.0"
  }
}
